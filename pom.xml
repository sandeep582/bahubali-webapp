<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.lync.bahubali-web</groupId>
  <artifactId>bahubali-web</artifactId>
  <packaging>war</packaging>
  <version>1.0-SNAPSHOT</version>
  <name>bahubali-web Maven Webapp</name>
  <url>http://maven.apache.org</url>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <build>
    <finalName>bahubali-web</finalName>
  </build>
  <distributionManagement>
    <repository>
       <id>bahubaliRelRepo</id>
       <url>http://localhost:8081/nexus/content/repositories/bahubaliRelRepo/</url>
    </repository>

    <snapshotRepository>
      <id>bahubaliSnapRepo</id>
      <url>http://localhost:8081/nexus/content/repositories/bahubaliSnapRepo/</url>
    </snapshotRepository>
  </distributionManagement>
</project>
